{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Kambrium Documentation",
  "colors": {
    "primary": "#8470FF",
    "light": "#9C8CFF",
    "dark": "#5D47DE",
    "background": {
      "light": "#F9FAFB",
      "dark": "#111827"
    },
    "accent": "#67BFFF"
  },
  "font": {
    "family": "Inter",
    "weight": [
      "400",
      "500",
      "600",
      "700"
    ]
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.kambrium.com",
          "icon": "chart-line"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/kambrium",
          "icon": "discord"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@kambrium.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@kambrium.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.kambrium.com"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/kambrium",
      "linkedin": "https://linkedin.com/company/kambrium"
    }
  }
}